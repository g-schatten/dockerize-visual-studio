{
    "name": "JS ENV",
    "dockerFile": "Dockerfile",
    "appPort": [3000, 8080],
    "runArgs": ["-u", "node"],
    "settings": {
        "workbench.colorTheme": "cobalt2",
        "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe"
    },
    "postCreateCommand": "npm install",
    "extensions": [
        "esbenp.prettier-vscode",
        "wesbos.theme-cobalt2"
    ] 
}